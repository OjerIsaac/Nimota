{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom"],
    "module": "commonjs",
    "rootDir": "./",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true
  }
}
